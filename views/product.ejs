<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="product.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    <link rel="stylesheet" href="https://unpkg.com/swiper@8/swiper-bundle.min.css" />
    <title>Product Page</title>
  </head>
  <body>

    <div class="container">

        <!-- **************************8 ***************** -->
        <div class="container1">

            <div id="carouselExampleControls" class="objectimg carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                  <div class="carousel-item active">
                    <img src="images/128.jpeg" style="height:60vh" class="d-block w-100" alt="...">
                  </div>
                  <div class="carousel-item">
                    <img src="images/127.jpeg" style="height:60vh" class="d-block w-100" alt="...">
                  </div>
                  <div class="carousel-item">
                    <img src="images/126.jpeg" style="height:60vh" class="d-block w-100" alt="...">
                  </div>
                </div>
                <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="sr-only">Next</span>
                </a>
              </div>

        </div>


            <!-- **************************8 ***************** -->


            <div class="container2">

                <div class="style12 prize">
                    <div class="box">
                        <div class="price">
                            <div class="cost">
                                <h3 ><b><%= card.price %></b></h3>
                            </div>
                            
                            <div class="prizeicon">
                                <p><i class="fa-solid fa-share-nodes share"></i> </p>
                                <p> <i class="fa-regular fa-heart"></i> </p>
                            </div>
                        </div>
                        <span><%= card.model_name %> </span>
                        <div class="location">
                            <p><%= card.address %></p>
                            <p>Today</p>
                        </div>
                        <a class="btn chatbtn" href="#" role="button">Buy</a>
                        <a class="btn chatbtn" href="#" role="button">Rent</a>
                    </div>
                   
                </div>

                <div class="style12 sellersdetails">
                    <h4 class="Seller_description"> <b>Seller Description</b> </h4>
                <div class="seller_detail">
                    <div>
                        <h2> AsKAki </h2>
                        <p>Email : askaki123@gmail.com</p>
                        <p>Member since Nov 2021</p>
                    </div>
                </div>
                <a class="btn chatbtn" href="#" role="button">Chat with Seller</a>
                </div>

            </div>

        </div>

    </div>

<!-- **************************8 ***************** -->
        <div class="container3">

            <div class="details detailstyle">
                <div class="objectname">
                    <h3><b><%= card.model_name %></b></h3>
                </div><br><br>
                <div class="detailsinfo">
                    <P>Year Of Purchase : <%= card.date_of_purchase %></P>
                    <P>Physical Condition : <%= card.physical_condition %></P>
                    <P>warranty Status : <%= card.warranty %></P>
                    
                </div>
            
            </div>

            <div class="features detailstyle">

                <div>
                    <h3><b>Product Details</b></h3><br><br>
                </div>
                <div class="featureslist">
                    <p>Color :<%= card.color %></p>
                    <p>Dimensions : <%= card.dimension %></p>
                    <p>Quantity : <%= card.quantity %></p>
                </div>



            </div>


        </div>
        


    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
  </body>
</html>